{
  "name": "skystage-platform",
  "version": "1.0.0",
  "description": "Complete SkyStage drone show platform with modular database architecture",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "next dev -H 0.0.0.0",
    "build": "next build",
    "start": "next start -H 0.0.0.0 -p 3000",
    "lint": "next lint",
    "db:init": "node scripts/init-db.js",
    "db:seed": "node scripts/seed-db.js",
    "db:migrate": "node scripts/migrate-db.js",
    "db:migrate:supabase": "tsx scripts/migrate-supabase.ts",
    "db:migrate:sqlite": "tsx scripts/migrate-sqlite.ts",
    "db:seed:supabase": "tsx scripts/seed-supabase.ts",
    "db:init-users": "tsx scripts/init-default-users.ts",
    "db:test": "tsx scripts/test-database.ts",
    "db:health": "curl -s http://localhost:3000/api/admin/database/health | jq .",
    "setup:supabase": "npm run db:migrate:supabase && npm run db:seed:supabase",
    "setup:sqlite": "npm run db:migrate:sqlite && npm run db:init-users",
    "setup:quick": "npm run db:init-users",
    "test:database": "npm run db:test",
    "check:env": "tsx -e \"console.log('Database Provider:', process.env.DATABASE_PROVIDER || 'sqlite'); console.log('Supabase URL:', process.env.NEXT_PUBLIC_SUPABASE_URL ? 'Set' : 'Not Set');\"",
    "scrape:help": "bun scripts/scrape-help.ts"
  },
